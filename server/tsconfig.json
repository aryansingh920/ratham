{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "allowJs": true, // Add this line
    "resolveJsonModule": true // Add this line if you need to import JSON files
  },
  "include": ["src/**/*.ts", "src/**/*.js", "src/server.ts"], // Add .js files here
  "exclude": ["node_modules"]
}
